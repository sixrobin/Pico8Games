function draw_cut()
	local col=6
	if cut_timer>-1 then
		col=1
	end
	
	for i=0,64 do
		pset(i*3,cut_h,col)
	end
	
	-- draw knife.
	spr(101,knife_x,cut_h-8,2,2,knife_flipx)
end

function draw_cut_clamps()
 	for i=0,64 do
			pset(i*3,cut_minh,1)
			pset(i*3,cut_maxh,1)
		end
 --line(0,cut_minh,127,cut_minh,1)
 --line(0,cut_maxh,127,cut_maxh,1)
end


